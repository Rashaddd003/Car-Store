@page
@model Car_Store.Pages.FilterPageModel
@{
    Layout = "_Layout";
}


<link rel="stylesheet" href="~/css/filter.css" asp-append-version="true">
<!-- Info before filter and cars -->
<div class="pre-filter">
    <select name="cars" id="cars">
        <option value="1">Default sorting</option>
        <option value="2">Sort by popularity</option>
        <option value="mercedes">sort by average rating</option>
        <option value="audi">sort by latest</option>
        <option value="audi">sort by price low to high</option>
        <option value="audi">sort by price high to low</option>

    </select>
</div>
<!-- end  -->
<!-- Start Best Offer -->
<div class="main-container d-flex ">
    <div class="filter">
        <!-- Start Filter -->
        <div class="wrapper">
            <div class="wd-wpb">
                <div class="widget widget sidebar-widget woocommerce widget_price_filter">
                    <h5 class="widget-title">Filter by price</h5>
                    <form>
                        <div class="price_slider_wrapper">
                            <div class="price_slider ui-slider ui-corner-all ui-slider-horizontal ui-widget ui-widget-content">
                                <div class="slider-track"></div>
                                <input type="range" min="100" max="10000" value="1000" id="slider-1" oninput="slideOne()">
                                <input type="range" min="100" max="10000" value="5000" id="slider-2" oninput="slideTwo()">
                            </div>
                            <div class="price_slider_amount">
                                <input type="text" id="min_price" name="min_price" value="2980" data-min="490" placeholder="Min price" style="display: none;">
                                <input type="text" id="max_price" name="max_price" value = "2398" data-max="13500" placeholder="Max price" style="display: none;">
                                <button type="submit" class="filter-button">Filter</button>
                                <div class="price_label">
                                    Price: <span class="from" id="range1">$2,980</span> — <span class="to" id="range2">$12,650</span>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div id="woodmart-woocommerce-layered-nav-2" class="wd-widget-collapse widget widget sidebar-widget woodmart-woocommerce-layered-nav">
                    <div class="clickable-div"> <h5 class="widget-title">Filter by Color</h5> <i class="fa-solid fa-chevron-down select-arrow"></i> </div>
                    <div class="wd-scroll" style="display: none;">
                        <div class="mini-option">
                            <span>LG</span>
                            <input type="checkbox" class="tickbox" id="option1" onchange="changeTickbox(this)">
                            <label for="option1"></label>
                        </div>
                        <div class="mini-option">
                            <span>LG</span>
                            <input type="checkbox" class="tickbox" id="option2" onchange="changeTickbox(this)">
                            <label for="option2"></label>
                        </div>
                        <div class="mini-option">
                            <span>LG</span>
                            <input type="checkbox" class="tickbox" id="option3" onchange="changeTickbox(this)">
                            <label for="option3"></label>
                        </div>
                        <div class="mini-option">
                            <span>LG</span>
                            <input type="checkbox" class="tickbox" id="option4" onchange="changeTickbox(this)">
                            <label for="option4"></label>
                        </div>
                    </div>
                </div>
                <div id="woodmart-woocommerce-layered-nav-2" class="wd-widget-collapse widget widget sidebar-widget woodmart-woocommerce-layered-nav">
                    <div class="clickable-div"> <h5 class="widget-title">Filter by Color</h5> <i class="fa-solid fa-chevron-down select-arrow"></i> </div>
                    <div class="wd-scroll" style="display: none;">
                        <div class="mini-option">
                            <span>LG</span>
                            <input type="checkbox" class="tickbox" id="option5" onchange="changeTickbox(this)">
                            <label for="option5"></label>
                        </div>
